{
    "name": "serafimpikalov/telegram-bot",
    "type": "library",
    "description": "PHP telegram bot by LONGMAN (https://github.com/akalongman/php-telegram-bot) with modification by me for own using ",
    "keywords": ["telegram", "bot", "api"],
    "license": "MIT",
    "homepage": "https://github.com/SerafimPikalov/php-telegram-bot/",
    "authors": [
        {
            "name": "Serafim Pikalov",
            "email": "serafim.pikalov@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "ext-pdo": "*",
        "ext-curl": "*",
        "ext-mbstring": "*",
        "monolog/monolog": "^1.19",
        "guzzlehttp/guzzle": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Longman\\TelegramBot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Longman\\TelegramBot\\Tests\\Unit\\": "tests/unit"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "squizlabs/php_codesniffer": "~2.5"
    }
}
